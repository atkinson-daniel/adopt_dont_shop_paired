<h2>New Shelter</h2>

<%= form_tag "/shelters/" do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>
  <%= label_tag :address %>
  <%= text_field_tag :address %><br>
  <%= label_tag :city %>
  <%= text_field_tag :city %><br>
  <%= label_tag :state %>
  <%= text_field_tag :state %><br>
  <%= label_tag :zip %>
  <%= number_field_tag :zip %><br><br>
  <%= submit_tag "Create Shelter" %>
<% end %>

<br><h5><%= link_to "Go Back", :back %></h5>
